{
  "app": {
    "apiVersion": {
      "target": 5,
      "compatible": 4
    },
    "bundleName": "com.samlach22.watchtronome",
    "vendor": "samlach22",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "name": "default",
        "icon": "$media:icon",
        "label": "$string:entry_MainAbility",
        "type": "page"
      }
    ],
    "reqPermissions": [
      {
        "usedScene": {
          "when": "inuse"
        },
        "reason": "Load PNG files",
        "name": "ohos.permission.READ_MEDIA"
      }
    ],
    "js": [
      {
        "window": {
          "designWidth": 0
        },
        "name": "default",
        "pages": [
          "pages/index/index",
          "pages/RhythmPage/RhythmPage"
        ]
      }
    ]
  }
}